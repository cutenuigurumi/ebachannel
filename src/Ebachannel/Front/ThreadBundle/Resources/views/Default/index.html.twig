{% extends '::base.html.twig' %}

{% block title %}えばちゃんねる管理者画面{% endblock %}

{% block body %}
<p>
カテゴリ名：{{category.name}}
</p>
<table>
    <tr>
        <td>ID</td>
        <td>名前</td>
    </tr>
        {% for thread in thread %}
        <tr>
            <td>{{ thread.id }}</td>
            <td>{{ thread.name }}</td>
            
        </tr>
       {% endfor %}
</table>
<p>{{ nothing_message }}</p>
<a href="{{ path('front_thread_new', { 'category_id': category.id }) }}">新規作成</a>
{% endblock body %}
